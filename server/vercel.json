{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    { "source": "/register", "destination": "/client/signup.html" },
    { "source": "/login", "destination": "/client/login.html" },
    { "source": "/dashboard", "destination": "/client/dashboard.html" }
  ]
}
