{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/client/(.*)",
      "dest": "/client/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}